		"ls_source_q3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_source_q3'"
		},
		"sqltest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqltest'"
		},
		"ls_adls_projectdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://projectstorageaccadls.dfs.core.windows.net"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
